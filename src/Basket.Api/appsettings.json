{
  "ConnectionStrings": {
    "Redis": "host.docker.internal,port:6379,password=redis"
  },
  "Redis": {
    "InstanceName": "baskets-"
  },
  "Elasticsearch": {
    "Uri": "http://host.docker.internal:9200",
    "LogsSettings": {
      "IndexFormat": "app-basket-api-{0:yyyy.MM.dd}"
    }
  },
  "ElasticApm": {
    "ServerUrl": "http://host.docker.internal:8200",
    "ServiceName": "Basket API",
    "Environment": "Production"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
