{
  "ConnectionStrings": {
    "Redis": "host.docker.internal,port:6379,password=redis"
  },
  "Redis": {
    "InstanceName": "baskets-"
  },
  "UsersApiEndpoint": "http://host.docker.internal:8084/account",
  "Elasticsearch": {
    "Uri": "http://elastic:elastic1234@host.docker.internal:9200",
    "IndexFormat": "basket-api"
  },
  "ElasticApm": {
    "ServerUrl": "http://host.docker.internal:8200",
    "ServiceName": "basket-api",
    "Environment": "production"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}